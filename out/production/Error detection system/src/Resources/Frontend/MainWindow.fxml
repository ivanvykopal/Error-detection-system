      <Label text="Vyberte možnosť" textAlignment="CENTER">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button fx:id="button1" mnemonicParsing="false" onAction="#analysis1" prefHeight="31.0" prefWidth="300.0" text="Analyzovať jeden zdrojový kód" textAlignment="CENTER">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="button2" mnemonicParsing="false" onAction="#analysis2" prefHeight="31.0" prefWidth="300.0" text="Analyzovať viacero zdrojových kódov" textAlignment="CENTER">
